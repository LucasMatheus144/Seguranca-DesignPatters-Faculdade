@model EspacoPotencial.Areas.Cadastro.Models.Usuarios.desligamento_motivos_usuario

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>desligamento motivos usuario</h4>
<hr />
<div class="row">
